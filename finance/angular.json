{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "finance": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/finance",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              },
              "src/assets",
              "src/robots.txt",
              "src/sitemap.xml",
              "src/ads.txt"
            ],
            "styles": [
              "src/styles.scss",
              "node_modules/primeicons/primeicons.css",
              "node_modules/primeng/resources/themes/lara-light-blue/theme.css",
              "node_modules/primeng/resources/primeng.min.css"
            ],
            "scripts": [
              "node_modules/chart.js/dist/chart.js"
            ],
            "server": "src/main.server.ts",
            "prerender": true,
            "ssr": {
              "entry": "server.ts"
            },
            "allowedCommonJsDependencies": [
              "mathjs",
              "seedrandom",
              "tiny-emitter",
              "escape-latex",
              "javascript-natural-sort",
              "@babel/runtime"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "3MB",
                  "maximumError": "4MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kB",
                  "maximumError": "4kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "finance:build:production"
            },
            "development": {
              "buildTarget": "finance:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "src/styles.scss",
              "node_modules/primeng/resources/themes/nova-light/theme.css",
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/primeicons/primeicons.css"
            ],
            "scripts": []
          }
        },
        "prerender": {
          "builder": "@angular-devkit/build-angular:prerender",
          "options": {
            "browserTarget": "finance:build:production",
            "serverTarget": "finance:server:production",
            "routes": [
              "/",
              "/rendement-locatif",
              "/budget-immobilier",
              "/investissement-locatif",
              "/cashflow-immobilier",
              "/calcul-emprunt",
              "/tri-immobilier",
              "/achat-vs-location",
              "/calcul-frais-de-notaire",
              "/interet-compose",
              "/lmnp-lmp-regime-fiscal",
              "/simulateur-impot-revenue",
              "/simulateur-brut-net",
              "/comparateur-sci-impot",
              "/simulateur-plus-value-immobiliere",
              "/simulateur-impot-fortune-immobiliere",
              "/simulateur-independance-financiere",
              "/retraite",
              "/prime-activite",
              "/calculatrice",
              "/article/pea-vs-assurance-vie",
              "/article/pea",
              "/article/assurance-vie",
              "/article/perco",
              "/article/per",
              "/article/cto",
              "/article/assurance-vie-luxembourgeoise",
              "/article/credit-lombard",
              "/article/etf",
              "/simulateur-location-nue-regime",
              "/simulateur-apport-vs-emprunt",
              "/airbnb-vs-location",
              "/simulateur-dividend-fire"
            ]
          },
          "configurations": {
            "production": {}
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/finance-server",
            "main": "server.ts",
            "tsConfig": "tsconfig.server.json"
          },
          "configurations": {
            "production": {
              "outputPath": "dist/finance-server"
            }
          }
        }
      }
    }
  }
}